{
    "name": "andyts93/brt-api-wrapper",
    "description": "PHP wrapper for BRT Rest API service",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "Andyts93\\BrtApiWrapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrea Castellini",
            "email": "and1@live.it"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "vendor/bin/phpcs --extensions=php src tests"
    },
    "require": {
        "php": "^8.1 || ^8.2 || ^8.3 || ^8.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^11.4",
        "squizlabs/php_codesniffer": "^3.9",
        "phpstan/phpstan": "^1.11"
    }
}
